<configs>
	<insertafter xpath="/items/item[@name]/property[1]">
		<property name="CreativeMode" value="None"/>
	</insertafter>
	<set xpath="/items/item//property[@name='CreativeMode']/@value">None</set>
	
	<insertAfter xpath="/items/item[@name='ammoBundleMaster']">
	<item name="Backpack">
		<property name="CreativeMode" value="None"/>
		<property name="CustomIcon" value="backpackMedium"/>
		<property name="ItemTypeIcon" value="bundle"/>
		<property name="HoldType" value="45"/>
		<property name="Meshfile" value="@:Other/Items/Misc/sackPrefab.prefab"/>
		<property name="DropMeshfile" value="@:Other/Items/Misc/sack_droppedPrefab.prefab"/>
		<property name="Material" value="Mmetal"/>
		<property name="Stacknumber" value="1"/> <!-- STK ammobundle high -->
		<property name="EconomicValue" value="0"/>
		<property class="Action0">
			<property name="Class" value="OpenBundle"/>
			<property name="Delay" value="0"/>
			<property name="Use_time" value="0"/>
			<property name="Sound_start" value="UseActions/open_backpack"/>
			<property name="Create_item" value="meleeToolRepairT0StoneAxe,drinkJarBoiledWater,meleeWpnBladeT0BoneKnife,foodCanChili,medicalFirstAidBandage,meleeToolTorch"/>
			<property name="Create_item_count" value="6,2,1"/>
		</property>
	</item>	
	</insertAfter>
	
	<set xpath="/items/item[@name='foodPumpkinBread']/effect_group/triggered_effect[@cvar='$foodAmountAdd']/@value">22</set>
	<set xpath="/items/item[@name='foodPumpkinBread']/effect_group/triggered_effect[@cvar='foodHealthAmount']/@value">11</set>
	<set xpath="/items/item[@name='foodEgg']/property[@name='EconomicValue']/@value">30</set>
	
	<set xpath="//item[@name='resourceCoal']//property[@name='Group']/@value">Resources,Chemicals,CFChemicals</set>
	
	<set xpath="//item[@name='resourceWaterFilter']//property[@name='Stacknumber']/@value">1</set>
</configs>