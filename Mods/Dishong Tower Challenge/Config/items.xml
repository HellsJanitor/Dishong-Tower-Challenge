<configs>
	<insertafter xpath="/items/item[@name]/property[1]">
		<property name="CreativeMode" value="None"/>
	</insertafter>
	<set xpath="/items/item//property[@name='CreativeMode']/@value">None</set>
	
	<insertAfter xpath="/items/item[@name='ammoBundleMaster']">
	<item name="Backpack">
		<property name="CreativeMode" value="None"/>
		<property name="CustomIcon" value="backpackMedium"/>
		<property name="ItemTypeIcon" value="bundle"/>
		<property name="HoldType" value="45"/>
		<property name="Meshfile" value="@:Other/Items/Misc/sackPrefab.prefab"/>
		<property name="DropMeshfile" value="@:Other/Items/Misc/sack_droppedPrefab.prefab"/>
		<property name="Material" value="Mmetal"/>
		<property name="Stacknumber" value="1"/> <!-- STK ammobundle high -->
		<property name="EconomicValue" value="0"/>
		<property class="Action0">
			<property name="Class" value="OpenBundle"/>
			<property name="Delay" value="0"/>
			<property name="Use_time" value="0"/>
			<property name="Sound_start" value="UseActions/open_backpack"/>
			<property name="Create_item" value="meleeToolRepairT0StoneAxe,drinkJarBoiledWater,meleeWpnBladeT0BoneKnife,foodCanChili,medicalFirstAidBandage,meleeToolTorch"/>
			<property name="Create_item_count" value="6,2,1"/>
		</property>
	</item>	
	</insertAfter>
	
	<set xpath="/items/item[@name='foodPumpkinBread']/effect_group/triggered_effect[@cvar='$foodAmountAdd']/@value">22</set>
	<set xpath="/items/item[@name='foodPumpkinBread']/effect_group/triggered_effect[@cvar='foodHealthAmount']/@value">11</set>
	<set xpath="/items/item[@name='foodEgg']/property[@name='EconomicValue']/@value">30</set>
	
	<set xpath="//item[@name='resourceCoal']//property[@name='Group']/@value">Resources,Chemicals,CFChemicals</set>
	<set xpath="//item[@name='resourceAcid']//property[@name='Group']/@value">Resources,Chemicals,CFChemicals</set>
	<insertbefore xpath="//item[@name='resourceAcid']//property[1]">
		<property name="UnlockedBy" value="craftingVehicles"/>
	</insertbefore>
	
	<insertafter xpath="//item[@name='resourceAcid']">
	<item name="resourceBatteryAcid">
		<property name="CustomIcon" value="resourceAcid"/>
		<property name="CreativeMode" value="None"/>
		<property name="HoldType" value="45"/>
		<property name="Meshfile" value="@:Other/Items/Misc/sackPrefab.prefab"/>
		<property name="DropMeshfile" value="@:Other/Items/Misc/sack_droppedPrefab.prefab"/>
		<property name="DescriptionKey" value="resourceBatteryAcidDesc"/>
		<property name="Material" value="MresourceAcid"/>
		<property name="Weight" value="0"/>
		<property name="Stacknumber" value="100"/> <!-- STK resource -->
		<property name="EconomicValue" value="140"/>
		<property name="EconomicBundleSize" value="1"/>
		<property name="Group" value="Resources,Chemicals"/>
	</item>
	</insertafter>

	<!-- <property name="DescriptionKey" value="armorLeatherGroupDesc"/> -->
	
	<set xpath="//item[@name='resourceWaterFilter']//property[@name='Stacknumber']/@value">1</set>
	<insertafter xpath="//item[@name='drugAntibiotics']">
	<item name="drugAntibioticsDT">
		<property name="CreativeMode" value="None"/>
		<property name="CustomIcon" value="drugAntibiotics"/>
		<property name="HoldType" value="16"/>
		<property name="Meshfile" value="@:Other/Items/Health/painkillersPrefab.prefab"/>
		<property name="DropMeshfile" value="@:Other/Items/Misc/sack_droppedPrefab.prefab"/>
		<property name="Material" value="Morganic"/>
		<property name="EconomicValue" value="240"/>
		<property name="Stacknumber" value="10"/> <!-- STK loot -->
		<property name="UnlockedBy" value="craftingMedical"/><!-- perkPhysician,drugAntibioticsSchematic, -->
		<property name="DisplayType" value="cureInfection"/>
		<property name="Group" value="Science,CFChemicals,Medical,TCMedical"/>
		<property class="Action0">
			<property name="Class" value="Eat"/>
			<property name="Delay" value="1.0"/>
			<property name="Sound_start" value="player_painkillers"/>
		</property>
		<effect_group tiered="false">
			<display_value name="dCureInfection" value=".25"/>
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="ModifyCVar" cvar="$buffInfectionAddCurePerc" operation="add" value="25"/>
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" buff="buffInfectionAddCure"/>
		</effect_group>
	</item>
	</insertafter>
	
